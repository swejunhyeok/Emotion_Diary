<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.co.kangnam.seminar">

    <application
        android:label="Emotionary"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupActivity" android:label="SignupActivity"/>
        <activity android:name=".MainPageActivity" android:label="MainPageActivity"/>
        <activity android:name=".InputCalendarActivity"/>
        <activity android:name=".CommunityActivity"/>
        <activity android:name=".InputCommunityActivity"/>
        <activity android:name=".ShowCommunityActivity"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
</manifest>